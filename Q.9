class Solution {
    public boolean isPalindrome(int x) {
        int temp=x;
        if (x<0)
        {
            return false;
        }
        int sum=0;
        while(x>0)
        {
            int n=x%10;
            sum=(sum*10)+n;
            x=x/10;
        }
        if(sum==temp)
        {
            return true;
        }
        return false;
    }
}
